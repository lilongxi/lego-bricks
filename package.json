    "husky": "^4.3.8",
    "rimraf": "^3.0.2",
    "rollup": "^2.44.0",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.30.0",
    "rollup-plugin-vue": "^6.0.0",
    "tslib": "^2.2.0",
    "typescript": "^3.9.9",
    "vue": "^3.0.0",
    "vue-jest": "^5.0.0-0"
  },
  "peerDependencies": {
    "vue": "^3.0.0"
